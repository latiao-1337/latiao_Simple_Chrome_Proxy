{
    "manifest_version": 3,
    "name": "Latiao Simple Proxy",
    "version": "1.0",
    "description": "支持 HTTP/HTTPS/SOCKS4/5/QUIC 代理",
    "permissions": [
        "proxy",
        "storage",
        "notifications"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}